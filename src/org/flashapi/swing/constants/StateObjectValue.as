package org.flashapi.swing.constants {
	
	// -----------------------------------------------------------
	// StateObjectValue.as
	// -----------------------------------------------------------

	/**
	* @author Pascal ECHEMANN
	* @version 1.0.0, 11/11/2010 11:16
	* @see http://www.flashapi.org/
	*/

	import org.flashapi.swing.core.DeniedConstructorAccess;
	
	/**
	 * 	The <code>StateObjectValue</code> defines the constant value to initialize
	 * 	<code>StateObject</code> properties.
	 * 
	 * 	@see org.flashapi.swing.state.StateObject
	 * 
	 * 	@langversion ActionScript 3.0
	 * 	@playerversion Flash Player 9
	 * 	@productversion SPAS 3.0 alpha
	 */	
	public class StateObjectValue {
		
		//--------------------------------------------------------------------------
		//
		// Constructor
		//
		//--------------------------------------------------------------------------
		
		/**
		 * 	@private
		 * 
		 * 	Constructor.
		 * 
		 *  @throws 	org.flashapi.swing.exceptions.DeniedConstructorAccess
		 * 				A DeniedConstructorAccess if you try to create a new
		 * 				StateObjectValue instance.
		 */
		public function StateObjectValue() {
			super();
			new DeniedConstructorAccess(this);
		}
		
		//--------------------------------------------------------------------------
		//
		// Public constants
		//
		//--------------------------------------------------------------------------
		
		/**
		 * 	Defines the default value of a <code>StateObject</code> property.
		 */
		public static const NONE:String = "none";
	}
}